{
  "name": "@call-a3/eslint-config",
  "version": "1.1.1",
  "description": "The eslint config preferred by Adriaan Callaerts",
  "main": "index.js",
  "scripts": {
    "test": "echo 'No tests in place yet for this shareable eslint config'",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/call-a3/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": {
    "name": "Adriaan Callaerts",
    "email": "adriaan.callaerts@gmail.com",
    "url": "https://www.adriaan.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/call-a3/eslint-config/issues"
  },
  "homepage": "https://github.com/call-a3/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">=6.0.1",
    "eslint-plugin-prettier": ">=3.0.0",
    "eslint-plugin-standard": ">=4.0.0"
  },
  "dependencies": {
    "babel-eslint": ">=10.0.2",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^15.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "semantic-release": "^17.0.0"
  },
  "release": {
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
